Arrow = class extends Trap
{
	
	constructor(x,y,sx,sy,rotation)
	{
		super(x,y);
		this.size = size;
		this.speedX = sx;
		this.speedY = sy;
		this.squares = [];
		this.squares.push(new Square(this.x,this.y,"#FF0000","arrow",rotation))l
		this.rotation = rotation;
	}
	collision(player,"shooter")
	{	
		return super.collision(player);
	}
	update()
	{
		this.x += this.speedX;
		this.y += this.speedY;
	
	}	
	joinSpinner(spinner)
	{
		
		
	}
	checkForMover()
	{
		
	}
}
